    </mapping>
    <mapping class="com.xumiao.ws.vo.GetBlankUserResponse" name="getBlankUserResponse">
        <structure map-as="ns1:User" get-method="getReturn" set-method="setReturn" usage="optional" name="return"/>
    </mapping>
</binding>