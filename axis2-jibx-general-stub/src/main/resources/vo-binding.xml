    </mapping>
    <mapping abstract="true" type-name="ns1:User" class="com.xumiao.ws.vo.User">
        <value style="element" name="age" get-method="getAge" set-method="setAge" usage="optional"/>
        <value style="element" name="name" get-method="getName" set-method="setName" usage="optional"/>
    </mapping>
    <mapping class="com.xumiao.ws.vo.GetUserInfoResponse" name="getUserInfoResponse">
        <value style="element" name="return" get-method="getReturn" set-method="setReturn" usage="optional"/>
    </mapping>
    <mapping class="com.xumiao.ws.vo.GetUser" name="getUser">
        <value style="element" name="args0" get-method="getArgs0" set-method="setArgs0" usage="optional"/>
